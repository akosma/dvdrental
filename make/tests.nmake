	-@erase "$(INTDIR)\vc70.idb"
	-@erase "$(INTDIR)\vc70.pdb"
	-@erase "$(OUTDIR)\tests.exe"
	-@erase "$(OUTDIR)\tests.ilk"
	-@erase "$(OUTDIR)\tests.pdb"

"$(OUTDIR)" :
    if not exist "$(OUTDIR)/$(NULL)" mkdir "$(OUTDIR)"

CPP=cl.exe
CPP_PROJ=/Od /I "C:\cppunit-1.12.0\include" /D "WIN32" /D "_DEBUG" /D "_CONSOLE" /D "CPPUNIT_DLL" /D "_MBCS" /Gm /EHsc /RTC1 /MDd /Fo"$(OUTDIR)/" /Fd"$(OUTDIR)/vc70.pdb" /W3 /nologo /c /Wp64 /ZI /TP

