# Microsoft Developer Studio NMAKE File
!IF "$(CFG)" == ""
CFG=debug
!MESSAGE No configuration specified. Defaulting to debug.
!ENDIF

!IF "$(CFG)" != "release" && "$(CFG)" != "debug"
!MESSAGE Invalid configuration "$(CFG)" specified.
!MESSAGE You can specify a configuration when running NMAKE
!MESSAGE by defining the macro CFG on the command line. For example:
!MESSAGE
!MESSAGE NMAKE /f "unittests.mak" CFG="debug"
!MESSAGE
!MESSAGE Possible choices for configuration are:
!MESSAGE
!MESSAGE "release" (based on "Win32 (x86) Console Application")
!MESSAGE "debug" (based on "Win32 (x86) Console Application")
!MESSAGE
!ERROR An invalid configuration is specified.
!ENDIF

!IF "$(OS)" == "Windows_NT"
NULL=
!ELSE
NULL=nul
!ENDIF


# ======================================
# =========== RELEASE ==================
# ======================================

!IF  "$(CFG)" == "release"

OUTDIR=.\Release
INTDIR=.\Release
# Begin Custom Macros
OutDir=.\Release
# End Custom Macros

ALL : "$(OUTDIR)\unittests.exe"


CLEAN :
	-@erase "$(INTDIR)\dataaccesslayer.obj"
	-@erase "$(INTDIR)\dataaccesslayertest.obj"
	-@erase "$(INTDIR)\date.obj"
	-@erase "$(INTDIR)\datetest.obj"
	-@erase "$(INTDIR)\persistable.obj"
	-@erase "$(INTDIR)\person.obj"
	-@erase "$(INTDIR)\persontest.obj"
	-@erase "$(INTDIR)\unittests.obj"
	-@erase "$(INTDIR)\vc60.idb"
	-@erase "$(OUTDIR)\unittests.exe"

"$(OUTDIR)" :
    if not exist "$(OUTDIR)/$(NULL)" mkdir "$(OUTDIR)"

CPP=cl.exe
CPP_PROJ=/O2 /I "C:\sqlite-3.3.8" /I "C:\cppunit-1.12.0\include" /D "WIN32" /D "NDEBUG" /D "_CONSOLE" /D "CPPUNIT_DLL" /D "_MBCS" /FD /EHsc /MD /GS /Fo"Release/" /Fd"Release/vc70.pdb" /W3 /nologo /c /Wp64 /Zi /TP

.c{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cpp{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cxx{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.c{$(INTDIR)}.sbr::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cpp{$(INTDIR)}.sbr::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cxx{$(INTDIR)}.sbr::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

RSC=rc.exe
BSC32=bscmake.exe
BSC32_FLAGS=/nologo /o"$(OUTDIR)\unittests.bsc"
BSC32_SBRS= \

LINK32=link.exe
LINK32_FLAGS=/OUT:"Release/unittests.exe" /INCREMENTAL:NO /NOLOGO /LIBPATH:"C:\cppunit-1.12.0\lib" /LIBPATH:"C:\sqlite-3.3.8" /DEBUG /PDB:"Release/unittests.pdb" /SUBSYSTEM:CONSOLE /OPT:REF /OPT:ICF /MACHINE:X86 sqlite3.lib cppunit_dll.lib  kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib
LINK32_OBJS= \
	"$(INTDIR)\unittests.obj" \
	"$(INTDIR)\dataaccesslayertest.obj" \
	"$(INTDIR)\datetest.obj" \
	"$(INTDIR)\dataaccesslayer.obj" \
	"$(INTDIR)\person.obj" \
	"$(INTDIR)\persontest.obj" \
	"$(INTDIR)\date.obj" \
	"$(INTDIR)\persistable.obj"

"$(OUTDIR)\unittests.exe" : "$(OUTDIR)" $(DEF_FILE) $(LINK32_OBJS)
    $(LINK32) @<<
  $(LINK32_FLAGS) $(LINK32_OBJS)
<<


# ======================================
# ===========  DEBUG  ==================
# ======================================



!ELSEIF  "$(CFG)" == "debug"

OUTDIR=.\Debug
INTDIR=.\Debug
# Begin Custom Macros
OutDir=.\Debug
# End Custom Macros

ALL : "$(OUTDIR)\unittests.exe"


CLEAN :
	-@erase "$(INTDIR)\dataaccesslayer.obj"
	-@erase "$(INTDIR)\dataaccesslayertest.obj"
	-@erase "$(INTDIR)\date.obj"
	-@erase "$(INTDIR)\datetest.obj"
	-@erase "$(INTDIR)\persistable.obj"
	-@erase "$(INTDIR)\person.obj"
	-@erase "$(INTDIR)\persontest.obj"
	-@erase "$(INTDIR)\unittests.obj"
	-@erase "$(INTDIR)\vc60.idb"
	-@erase "$(INTDIR)\vc60.pdb"
	-@erase "$(OUTDIR)\unittests.exe"
	-@erase "$(OUTDIR)\unittests.ilk"
	-@erase "$(OUTDIR)\unittests.pdb"

"$(OUTDIR)" :
    if not exist "$(OUTDIR)/$(NULL)" mkdir "$(OUTDIR)"

CPP=cl.exe
CPP_PROJ=/Od /I "C:\sqlite-3.3.8" /I "C:\cppunit-1.12.0\include" /D "WIN32" /D "_DEBUG" /D "_CONSOLE" /D "CPPUNIT_DLL" /D "_MBCS" /Gm /EHsc /RTC1 /MDd /Fo"Debug/" /Fd"Debug/vc70.pdb" /W3 /nologo /c /Wp64 /ZI /TP

.c{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cpp{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cxx{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.c{$(INTDIR)}.sbr::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cpp{$(INTDIR)}.sbr::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

.cxx{$(INTDIR)}.sbr::
   $(CPP) @<<
   $(CPP_PROJ) $<
<<

RSC=rc.exe
BSC32=bscmake.exe
BSC32_FLAGS=/nologo /o"$(OUTDIR)\unittests.bsc"
BSC32_SBRS= \

LINK32=link.exe
LINK32_FLAGS=/OUT:"Debug/unittests.exe" /INCREMENTAL /NOLOGO /LIBPATH:"C:\cppunit-1.12.0\lib" /LIBPATH:"C:\sqlite-3.3.8" /DEBUG /PDB:"Debug/unittests.pdb" /SUBSYSTEM:CONSOLE /MACHINE:X86 sqlite3.lib cppunitd_dll.lib  kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib
LINK32_OBJS= \
	"$(INTDIR)\unittests.obj" \
	"$(INTDIR)\dataaccesslayertest.obj" \
	"$(INTDIR)\datetest.obj" \
	"$(INTDIR)\dataaccesslayer.obj" \
	"$(INTDIR)\person.obj" \
	"$(INTDIR)\persontest.obj" \
	"$(INTDIR)\date.obj" \
	"$(INTDIR)\persistable.obj"

"$(OUTDIR)\unittests.exe" : "$(OUTDIR)" $(DEF_FILE) $(LINK32_OBJS)
    $(LINK32) @<<
  $(LINK32_FLAGS) $(LINK32_OBJS)
<<

!ENDIF



!IF "$(CFG)" == "release" || "$(CFG)" == "debug"
SOURCE=..\src\data\dataaccesslayer.cpp

"$(INTDIR)\dataaccesslayer.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)


SOURCE=..\src\tests\dataaccesslayertest.cpp

"$(INTDIR)\dataaccesslayertest.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)


SOURCE=..\src\business\date.cpp

"$(INTDIR)\date.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)


SOURCE=..\src\tests\datetest.cpp

"$(INTDIR)\datetest.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)


SOURCE=..\src\business\persistable.cpp

"$(INTDIR)\persistable.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)


SOURCE=..\src\business\person.cpp

"$(INTDIR)\person.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)

SOURCE=..\src\tests\persontest.cpp

"$(INTDIR)\persontest.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)


SOURCE=..\src\tests\unittests.cpp

"$(INTDIR)\unittests.obj" : $(SOURCE) "$(INTDIR)"
	$(CPP) $(CPP_PROJ) $(SOURCE)



!ENDIF

