LINK32_FLAGS=/OUT:"Debug\BirthdayCard.exe" /INCREMENTAL:NO /NOLOGO /LIBPATH:"C:\wxWidgets-2.6.3\lib\\" /LIBPATH:"C:\sqlite-3.3.8" /DEBUG /PDB:"Debug/BirthdayCard.pdb" /SUBSYSTEM:WINDOWS /MACHINE:X86 wxmswd.lib winmm.lib comctl32.lib rpcrt4.lib wsock32.lib odbc32.lib sqlite3.lib  kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib
LINK32_OBJS= \
	"$(INTDIR)\person.obj" \
	"$(INTDIR)\date.obj" \
	"$(INTDIR)\persistable.obj" \
	"$(INTDIR)\dataaccesslayer.obj" \
	"$(INTDIR)\main.obj" \
	"$(INTDIR)\controller.obj" \
	"$(INTDIR)\birthdaycard.obj" \
	"$(INTDIR)\carddialog.obj" \
	"$(INTDIR)\datedialog.obj" \
	"$(INTDIR)\todayeventsdialog.obj"

"$(OUTDIR)\BirthdayCard.exe" : "$(OUTDIR)" $(DEF_FILE) $(LINK32_OBJS)
    $(LINK32) @<<
  $(LINK32_FLAGS) $(LINK32_OBJS)
<<

!ENDIF


